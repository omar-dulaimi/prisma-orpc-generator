{
  "name": "basic-from-generator",
  "version": "1.0.0",
  "description": "Example app generated by prisma-orpc-generator",
  "type": "module",
  "scripts": {
    "dev": "tsx watch --ignore 'src/generated/**' src/server.ts",
    "start": "node dist/server.js",
    "build": "tsc",
    "build:relaxed": "tsc --pretty false || true",
    "db:push": "prisma db push",
    "db:seed": "tsx src/seed.ts",
    "generate": "prisma generate",
    "test:generated": "vitest run --config /home/omar/Desktop/NewFolder/prisma-orpc-generator/examples/basic/src/generated/orpc/vitest.config.ts /home/omar/Desktop/NewFolder/prisma-orpc-generator/examples/basic/src/generated/orpc/tests/unit /home/omar/Desktop/NewFolder/prisma-orpc-generator/examples/basic/src/generated/orpc/tests/integration --environment node"
  },
  "dependencies": {
    "@orpc/openapi": "^1.8.8",
    "@orpc/server": "^1.8.7",
    "@prisma/client": "^6.16.1",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "orpc-shield": "^0.1.3",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.3.3",
    "prisma": "^6.16.0",
    "tsx": "^4.20.5",
    "typescript": "^5.7.2",
    "vite": "^7.1.5",
    "vitest": "^3.2.4"
  },
  "prisma": {
    "schema": "schema.prisma"
  }
}
